var
    f:text;
    n,i:longint; kt :boolean;
begin
    assign(f,'timso.int');
    reset(f);
    readln(f,n);
    close(f);
    assign(f,'timso.out');
    rewrite(f);
    kt:= false ;
    while kt = false do
    begin
        inc(n);
        for i:= 2 to n do
            if n mod i = 0 then
                break;
        if i = n then
            kt:= true ;
    end;
    write(f,n);
    close(f);
end.
