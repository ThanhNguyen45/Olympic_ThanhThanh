var
    n,i:longint;f:text;
begin
    assign(f,'TSNT.int');
    reset(f);
    readln(f,n);
    close(f);
    assign(f,'TSNT.out');
    rewrite(f);
    i:=1;
    while n <> 1 do
    begin
        inc(i);
        while n mod i = 0 do
        begin
            if n = i then
                write(f,i)
            else
                write(f,i,'.');
            n:= n div i;
        end;
    end;
    close(f);
end.
