var
    f:text;
    n,i,max,so,d : longint;
    function thuaso(x:longint):longint;
    var
        j,t:longint;
    begin
        j:=1;t:=0;
        while x <> 1 do
        begin
            inc(j);
            while x mod j = 0 do
            begin
                inc(t);
                x:= x div j;
            end;
        end;
        thuaso:= t;
    end;
begin
    assign(f,'Nhantu.int');
    reset(f);
    readln(f,n);
    close(f);
    assign(f,'Nhantu.out');
    rewrite(f);
    max:= 0;
    for i:= n downto 2 do
    begin
        d:=thuaso(i);
        if max < d then
        begin
            max:=d;
            so:=i;
        end;
    end;
    write(f,so);
    close(f);
end.
